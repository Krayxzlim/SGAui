<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/17.0.2" 
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.sga.controllers.ReportesController"
      spacing="10">
    <padding>
        <Insets top="10" right="10" bottom="10" left="10"/>
    </padding>

    <TableView fx:id="tableReportes" prefHeight="300">
        <columns>
            <TableColumn fx:id="colNombre" text="Nombre" prefWidth="200"/>
            <TableColumn fx:id="colValor" text="Valor" prefWidth="300"/>
        </columns>
    </TableView>

    <HBox spacing="10">
        <Button fx:id="btnRefrescar" text="Refrescar"/>
        <Button fx:id="btnExportar" text="Exportar Excel"/>
    </HBox>

</VBox>
